var fail=parseInt(info.fail);&#xD;
if(success>0){&#xD;
successDownload.set(&quot;disabled&quot;,false);&#xD;
}&#xD;
if(fail>0){&#xD;
failDownload.set(&quot;disabled&quot;,false);&#xD;
}&#xD;
view.get(&quot;#dsCommodityQuotedPrice&quot;).flushAsync();
</ClientEvent>
